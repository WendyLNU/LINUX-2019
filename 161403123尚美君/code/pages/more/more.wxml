<search-header></search-header>
<swiper class='swiper' indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" circular="{{true}}">
  <block wx:for="{{imgUrls}}" wx:key='*this'>
    <swiper-item>
      <image src="{{item}}" mode='widthFix' width="100%" />
    </swiper-item>
  </block>
</swiper>
<view class="more-page">
  <view class="row">
    <view class="right" bindtap='moreProduct'>更多...</view>
    <view class='sgin'></view>
    <view class="left">记忆库商城</view>
  </view>
  <view class="list">
    <view class='item' bindtap='checkProduct'>
      <view class='item-icon'>
        <image src='/img/icon/library-icon.png' mode='widthFix'></image>
      </view>
      <view class='item-text'>
        <text>Linux常用命令</text>
        <text class="info">作者：Jack</text>
      </view>
      <view class='item-button'>
        H O T
      </view>
    </view>
  </view>
  <view class="row">
    <view class='sgin'></view>
    <view class="left">最新资讯</view>
  </view>
  <view class="news">
    暂无资讯
  </view>
</view>